{
  "$schema":"http://json-schema.org/draft-04/schema#",
  "description":"Distributed Craigslist/Ebay/Amazon",
  "type": "object",
  "required": ["name","category","description","price"],
  "properties": {
    "name": {
      "type": "string",
      "title": "Title",
       "minLength": "3"
    },
    "category": {
      "type": "string",
      "title": "Category",
      "enum": [
        "Antiques",
        "Appliances",
        "Arts & Crafts",
        "Atvs, UTVs, Snowmobiles",
        "Auto Parts",
        "Auto Wheels & Tires",
        "Baby & Kid Stuff",
        "Barter",
        "Bicycle Parts",
        "Bicycles",
        "Boat Parts & Accessories",
        "Boats",
        "Books & Magazines",
        "Business",
        "Cars & Trucks",
        "CDs / DVDs / VHS",
        "Cell Phones",
        "Clothing & Accessories",
        "Collectibles",
        "Computer Parts",
        "Computers",
        "Electronics",
        "Farm & Garden",
        "Free Stuff",
        "Furniture",
        "Garage & Moving Sales",
        "General for Sale",
        "Health and Beauty",
        "Heavy Equipment",
        "Household Items",
        "Jewelry",
        "Materials",
        "Motorcycle Parts & Accessories",
        "Motorcycles/Scooters",
        "Musical Instruments",
        "Photo/Video",
        "Recreational Vehicles",
        "Sporting Goods",
        "Tickets",
        "Tools",
        "Toys & Games",
        "Trailers",
        "Video Gaming",
        "Wanted"
      ],
      "default": "Cars & Trucks"
    },
    "description": {
      "type": "string",
      "title": "Description"
    },
    "location": {
      "type": "string",
      "title": "Location"
    },
    "pictures": {
      "type": "array",
      "title": "Pictures",
      "items": {
        "type": "string",
        "format": "data-url"
      }
    },
    "price": {
      "type": "number",
      "title": "Price in Origin token"
    }
  }
}
