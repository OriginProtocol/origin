    "@origin/eventsource": "^0.1.0",
    "@origin/ipfs": "^0.1.0",
    "@origin/messaging-client": "^0.1.0",
    "@origin/mobile-bridge": "^0.1.0",
    "@origin/services": "^0.1.0",
    "@origin/validator": "^0.1.0",
    "@origin/web3-provider": "^0.1.0",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
