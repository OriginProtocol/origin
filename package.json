  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OriginProtocol/demo-dapp.git"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0-beta.46",
    "@babel/plugin-transform-destructuring": "^7.0.0-beta.46",
    "@babel/plugin-transform-object-assign": "^7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "@babel/register": "^7.0.0-beta.46",
    "@babel/runtime": "^7.0.0-beta.46",
    "babel-bridge": "^1.12.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "0.28.11",
    "dotenv": "^2.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.19.1",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "jest": "^22.4.3",
    "style-loader": "0.21.0",
    "webpack": "4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "3.1.3"
  },
  "dependencies": {
    "babel-loader": "^8.0.0-beta.2",
    "bootstrap": "^4.1.0",
    "bs58": "^4.0.1",
    "fetch": "^1.1.0",
    "jquery": "^3.3.1",
    "map-cache": "^0.2.2",
    "moment": "^2.22.1",
    "origin": "0.5.9",
    "popper.js": "^1.14.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-js-pagination": "^3.0.2",
    "react-jsonschema-form": "^1.0.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "install:dev": "npm install && npm link origin",
    "start": "webpack-dev-server --open",
    "build": "NODE_ENV=production webpack --progress",
    "test": "node scripts/test.js --env=jsdom"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-transform-destructuring",
      "@babel/plugin-transform-object-assign",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "impliedStrict": true
      }
    },
    "globals": {
      "web3": true
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/prop-types": "off",
      "react/no-children-prop": "off",
      "no-console": "off"
    }
  },
  "jest": {
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "./node_modules/",
      "./scripts/"
    ]
  }
}
