{
  "name": "@origin/token-transfer-server",
  "version": "1.0.0",
  "description": "Tools for distributing Origin tokens",
  "engines": {
    "node": "10.x"
  },
  "main": "app/app.js",
  "scripts": {
    "initdb": "npm run migrate && npm run seed && npm run vest",
    "lint": "eslint '**/*.js' && npm run prettier:check",
    "prettier": "prettier --write *.js \"src/**/*.js\"",
    "prettier:check": "prettier -c *.js \"src/**/*.js\"",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "start": "per-env",
    "start:development": "nodemon src/app.js",
    "start:production": "node src/app.js",
    "test": "NODE_ENV=test mocha -r @babel/register -r @babel/polyfill -t 10000 --exit",
    "vest": "node src/scripts/vest_grants.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@origin/contracts": "^0.8.6",
    "@origin/token": "^0.1.0",
    "bignumber.js": "8.0.2",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "connect-sqlite3": "^0.9.11",
    "console.table": "^0.10.0",
    "cors": "^2.8.5",
    "envkey": "^1.2.4",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "ip": "^1.1.5",
    "moment": "^2.22.2",
    "momentjs": "^2.0.0",
    "passport": "^0.4.0",
    "passport-google-token": "^0.1.2",
    "per-env": "^1.0.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-thunk": "^1.0.0",
    "react-validation": "^3.0.7",
    "redux": "^4.0.1",
    "sequelize": "^5.0.0-beta.14",
    "sequelize-cli": "^5.4.0",
    "sql-template-strings": "^2.2.2",
    "sqlite": "^3.0.0",
    "web3": "1.0.0-beta.34"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^3.0.0",
    "chai-moment": "^0.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "react-scripts": "^2.1.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
