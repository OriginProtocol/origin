{
  "name": "@origin/consistency",
  "version": "0.1.0",
  "description": "Database consistency checker",
  "private": true,
  "main": "src/index.js",
  "bin": {
    "origin-check": "src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "lint": "eslint . && npm run prettier:check",
    "prettier": "prettier --write *.js \"src/**/*.js\"",
    "prettier:check": "prettier -c *.js \"src/**/*.js\""
  },
  "license": "MIT",
  "dependencies": {
    "@origin/discovery": "^0.1.0",
    "@origin/graphql": "^0.1.0",
    "@origin/identity": "^0.1.0",
    "logplease": "1.2.15"
  },
  "devDependencies": {
    "prettier": "2.5.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
