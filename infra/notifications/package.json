    "apn": "^2.2.0",
    "cross-fetch": "^3.0.4",
    "dotenv": "^8.0.0",
    "envkey": "^1.2.6",
    "express": "^4.17.1",
    "firebase-admin": "^8.0.0",
    "lodash-es": "^4.17.11",
    "logplease": "^1.2.15",
    "mjml": "^4.5.1",
    "per-env": "^1.0.2",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.8.12",
