{
  "name": "@origin/linking",
  "version": "1.0.0",
  "description": "Mobile server for connecting with the mobile wallet",
  "main": "index.js",
  "engines": {
    "node": "9.11.1"
  },
  "scripts": {
    "build": "per-env",
    "build:production": "babel src -d dist",
    "lint": "eslint '**/*.js' && npm run prettier:check",
    "prettier": "prettier --write *.js \"**/*.js\"",
    "prettier:check": "prettier -c *.js \"**/*.js\"",
    "migrate": "sequelize db:migrate",
    "start": "per-env",
    "start:development": "nodemon -r @babel/register -r source-map-support src/index.js",
    "start:production": "node dist/index.js",
    "test": "NODE_ENV=test mocha -r @babel/register --timeout 10000 --exit"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "@origin/js": "^0.8.6",
    "apn": "^2.2.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.1",
    "dotenv": "^6.2.0",
    "eth-sig-util": "^2.1.1",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "express-useragent": "^1.0.12",
    "express-ws": "^4.0.0",
    "firebase-admin": "^7.0.0",
    "js-sha3": "^0.8.0",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "per-env": "^1.0.2",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "redis": "^2.8.0",
    "sequelize": "^5.0.0-beta.14",
    "sequelize-cli": "^5.4.0",
    "uuid": "^3.3.2",
    "web3": "1.0.0-beta.34"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "redis-mock": "^0.40.0",
    "supertest": "^3.3.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "sourceMaps": "both",
    "retainLines": true,
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
